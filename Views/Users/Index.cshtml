@model IEnumerable<Users>
 
  <hr />
  <a href="/Users/SignUp" class="btn btn-primary">Ãœye Ol</a>
  <table class="table table-bordered">
    <thead>
      <tr>
        <th style="width: 150px">@Html.DisplayNameFor(m=>m.UserNo)</th>
        <th>@Html.DisplayNameFor(m=>m.UserName)</th>
        <th>@Html.DisplayNameFor(m=>m.UserSurname)</th>
        <th style="width: 150px"></th>
      </tr>
    </thead>
    <tbody>
      @foreach(var item in Model) {
      <tr>
        <td>@Html.DisplayFor(modelItem=> item.UserNo)</td>
        <td>@Html.DisplayFor(modelItem=> item.UserName)</td>
        <td>@Html.DisplayFor(modelItem=> item.UserSurname)</td>
        <td>
          <a
            asp-action="Edit"
            asp-route-id="@item.UserNo"
            class="btn btn-sm btn-primary"
            >Edit</a
          >
          <a
            asp-action="Delete"
            asp-route-id="@item.UserNo"
            class="btn btn-sm btn-danger"
            >Delete</a
          >
        </td>
      </tr>
      }
    </tbody>
  </table></Users
>
